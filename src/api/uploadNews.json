{
    "url": "/admin/uploadNews",
    "description": "管理员发布新闻",
    "params": {
        "token": "鉴权token",
        "title": "新闻标题",
        "content": "内容列表，里面是字典",
        "content[].type": "内容类型，text/image/video",
        "content[].data": "文本内容，type为text时使用",
        "content[].url": "媒体链接，type为image或video时使用",
        "content[].preview": "媒体预览图片，type为image或video时使用"
    },
    "method": ["POST"],
    "response": {
        "statusCode": 200,
        "message": "成功",
        "id": "新闻ID"
    },
    "fail": {
        "statusCode": 425,
        "message": "发布失败",
        "id": null
    }
}
