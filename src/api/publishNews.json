{
    "url": "/admin/publishNews",
    "description": "管理员发布新闻",
    "params": {
        "token": "鉴权token",
        "title": "新闻标题",
        "content": [
            {
                "type": "text",
                "data": "新闻内容"
            },
            {
                "type": "image",
                "data": "图片链接，需要可以直接展示的url"
            },
            {
                "type": "video",
                "data": "视频链接，需要可以直接展示的url"
            }
        ]
    },
    "method": ["POST"],
    "response": {
        "statusCode": 200,
        "message": "成功",
        "id": "新闻ID"
    },
    "fail": {
        "statusCode": 425,
        "message": "发布失败",
        "id": null
    }
}