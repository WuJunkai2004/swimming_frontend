{
    "url": "/admin/uploadNews",
    "description": "管理员发布新闻",
    "params": {
        "token": "鉴权token",
        "title": "新闻标题",
        "content": [
            {
                "type": "text",
                "data": "新闻内容"
            },
            {
                "type": "image",
                "url": "图片链接，需要可以直接展示的url",
                "preview": "图片下方的描述"
            },
            {
                "type": "video",
                "url": "视频链接，需要可以直接展示的url",
                "preview": "视频预览图链接"
            }
        ]
    },
    "method": ["POST"],
    "response": {
        "statusCode": 200,
        "message": "成功",
        "id": "新闻ID"
    },
    "fail": {
        "statusCode": 425,
        "message": "发布失败",
        "id": null
    }
}
